{
  "name": "remix-toast",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "imports": {
    "#components*": "./app/components*",
    "#hooks*": "./app/hooks*",
    "#services*": "./app/services*",
    "#utils*": "./app/utils*",
    "#*": "./*"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:icons": "tsx ./scripts/build-icons.ts",
    "build:remix": "remix build",
    "predev": "npm run build:icons --silent",
    "dev": "remix dev --manual",
    "setup": "npm run build && npm run drizzle:migrate",
    "start": "remix-serve ./build/index.js",
    "drizzle:migrate": "drizzle-kit generate:sqlite && drizzle-kit push:sqlite",
    "drizzle:drop": "drizzle-kit drop",
    "drizzle:studio": "drizzle-kit studio",
    "lint": "eslint --fix --ext .tsx,ts app/",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@conform-to/react": "^0.9.0",
    "@conform-to/zod": "^0.9.0",
    "@remix-run/css-bundle": "^2.1.0",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "@remix-run/serve": "^2.1.0",
    "better-sqlite3": "^9.0.0",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.28.6",
    "invariant": "^2.2.4",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@klukies/eslint-config": "^1.0.0",
    "@remix-run/dev": "^2.1.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/better-sqlite3": "^7.6.6",
    "@types/fs-extra": "^11.0.3",
    "@types/invariant": "^2.2.36",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.38.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "node-html-parser": "^6.1.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "remix-flat-routes": "^0.6.1",
    "tailwindcss": "^3.3.4",
    "tsx": "^3.14.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
